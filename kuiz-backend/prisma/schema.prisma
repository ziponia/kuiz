datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           String    @default(uuid()) @id
  email        String    @unique
  profile_pic  String?
  createdAt    DateTime? @default(now())
  lastUpdateAt DateTime?

  @@map("tbl_user")
}